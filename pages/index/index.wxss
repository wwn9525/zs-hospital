/**index.wxss**/

view {
  font-family: "MicroSoft YaHei";
}

.fix::after {
  display: block;
  content: "clear";
  height: 0;
  clear: both;
  overflow: hidden;
  visibility: hidden;
}

.l {
  float: left;
}

.r {
  float: right;
}

.center {
  text-align: center;
}

.banner {
  height: 360rpx;
}

.slide-image {
  width: 750rpx;
}

.divider {
  height: 16px;
  background-color: #f1f1f3;
}

.quick-nav {
  display: flex;
  flex-flow: row wrap;
}

.quick-nav button {
  padding: 0;
  font-size: inherit;
  text-align: left;
  line-height: auto;
  border-radius: 0;
  background: none;
  overflow: hidden;
  display: inline;
}

.quick-nav-li {
  width: 373rpx;
  flex: 1;
  border-right: 4rpx solid #f4f5f6;
  border-bottom: 4rpx solid #f4f5f6;
}

.quick-nav:last-child .quick-nav-li {
  border-bottom: none;
}

.quick-nav-li:nth-child(even) {
  border-right: none;
}

.quick-nav-li .l {
  width: 204rpx;
  padding-left: 24rpx;
}

.quick-nav-li .l .main {
  font-size: 32rpx;
  line-height: 42rpx;
  margin-top: 30rpx;
}

.quick-nav-li .l .sub {
  font-size: 24rpx;
  margin-top: 16rpx;
  line-height: 32rpx;
  color: #828282;
}

.quick-nav-li .r {
  width: 142rpx;
  height: 148rpx;
  text-align: center;
}

.quick-nav-li .r image {
  width: 98rpx;
  margin-top: 30rpx;
}

.telephone {
  height: 156rpx;
  display: flex;
}

.tel-icon {
  flex: 6 1;
  height: 156rpx;
}

.tel-text {
  flex: 15 0;
  padding-left: 20rpx;
}

.tel-text text:first-child {
  font-size: 32rpx;
  color: #7f7f7f;
  display: block;
  margin-top: 28rpx;
}

.tel-text text:last-child {
  font-size: 32rpx;
  color: #2e2e2e;
  display: block;
  margin-top: 16rpx;
}

.tel-call {
  flex: 9 0;
  line-height: 156rpx;
}

.tel-call text {
  font-size: 28rpx;
  padding: 20rpx 40rpx;
  border: 1px solid #d9d9d9;
  border-radius: 50rpx;
}

.index_tl {
  width: 750rpx;
  height: 160rpx;
  display: block;
}

.index_tl_all button {
  display: inline;
  padding-left: 0;
  padding-right: 0;
  background: none;
  line-height: 0rpx;
}

.index_tl_all button:after {
  width: 0rpx;
  height: 0rpx;
}

.dis-list {
  display: flex;
}

.dis-list:last-child {
  padding-bottom: 30rpx;
}

.dis-list view {
  flex: 1;
  text-align: center;
}

.dis-list image {
  width: 96rpx;
  height: 96rpx;
  margin-top: 30rpx;
}

.dis-list text {
  display: block;
  text-align: center;
  height: 64rpx;
  line-height: 64rpx;
  font-size: 28rpx;
}

.dis-list view button {
  display: inline;
  padding-left: 0;
  padding-right: 0;
  background: none;
  line-height: 0rpx;
}

.dis-list view button:after {
  width: 0rpx;
  height: 0rpx;
}

.dynamic {
  margin-top: 30rpx;
}

.dynamic-li {
  display: flex;
}

.dynamic-li image {
  flex: 8;
  height: 200rpx;
}

.dynamic-li view {
  flex: 17;
}

.dynamic-li view text:first-child {
  margin-top: 8rpx;
  font-size: 32rpx;
  display: block;
}

.dynamic-li view text:last-child {
  margin-top: 24rpx;
  font-size: 28rpx;
  display: block;
  color: #828282;
}

.btn-icon {
  display: flex;
  margin: 20rpx auto 0;
}

.btn-icon-li {
  flex: 1;
  width: 331rpx;
  text-align: center;
}

.btn-icon view button {
  display: inline;
  padding-left: 0;
  padding-right: 0;
}

.btn-icon view button:after {
  width: 0rpx;
  height: 0rpx;
}

.btn {
  width: 331rpx;
  height: 95rpx;
  display: inline-block;
}

.footer {
  display: flex;
  margin: 30rpx 0;
}

.btn-view-li1 {
  flex: 1;
  border-right: 1px solid #ededed;
  padding-right: 10rpx;
  margin-right: 16rpx;
}

.btn-view-li2 {
  flex: 2;
}

.footer .btn1 {
  width: 225rpx;
  height: 176rpx;
  margin-left: 15rpx;
}

.btn-view button {
  background: none;
  border: none;
  line-height: auto;
  padding-right: 0;
  padding-left: 0;
  display: inline;
  line-height: 42rpx;
}

.footer .btn2 {
  width: 98rpx;
  height: 99rpx;
  border-botom: 1px solid #ededed;
  padding-bottom: 10rpx;
  margin-right: 15rpx;
}

.footer .btn3 {
  width: 98rpx;
  height: 99rpx;
  margin-right: 15rpx;
}

.btn-view-text1 {
  font-size: 27rpx;
  color: #8a8a8a;
  display: block;
  text-align: left;
}

.btn-view-text2 {
  font-size: 39rpx;
  color: #484848;
  display: block;
}

.btn-view_bor {
  border-bottom: 1px solid #ededed;
  padding-bottom: 10rpx;
  margin-bottom: 10rpx;
}

.btn-view {
  overflow: hidden;
}

.footer_bottom {
  background: #3a8afd;
  padding: 20rpx 20rpx 20rpx 0;
  overflow: hidden;
  text-align: center;
}

.footer_bottom image {
  width: 63rpx;
  height: 63rpx;
  padding-top: 5rpx;
}

.footer_bottom text {
  display: block;
  color: #fff;
  font-size: 28rpx;
  text-align: left;
}
